packages: Manifold.cabal
optional-packages: vendor/*/*.cabal
jobs: $ncpus
package Manifold
  ghc-options: -Weverything -Wno-missing-local-signatures -Wno-missing-import-lists -Wno-missing-export-lists -Wno-implicit-prelude -Wno-safe -Wno-unsafe -Wno-name-shadowing -Wno-monomorphism-restriction -Werror
